@page
@{
    ViewData["Title"] = "Feedbacks";
}

<form id="form" class="container" action="javascript:addFeedback()">
        <div class="form-group">
            <label for="fieldName">Name</label>
            <input required type="text" class="form-control" id="fieldName" placeholder="Your name">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Comment</label>
            <textarea rows="3" required class="form-control" id="fieldComment" placeholder="Your comment"></textarea>
        </div>
        <button id="btnsubmit" type="submit" class="btn btn-primary">Submit</button>
</form>

<div class="container" id="content"></div>

@section Scripts {
    <script src="~/js/FeedbackJS.js" asp-append-version="true"></script>
}